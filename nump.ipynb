   "source": [
    "from math import pi\n",
    "import numpy as np\n",
    "\n",
    "n = 2048\n",
    "freq = 100\n",
    "step = (freq * 2 * pi) / n\n",
    "sig = np.sin(np.arange(0, n) * step)\n",
    "\n",
    "f = np.fft.fft(sig)\n",
    "f = np.abs(f)\n",
    "\n",
    "peak_index = np.argmax(f)\n",
    "peak_value = f[peak_index]\n",
    "peak_index"
   ],
